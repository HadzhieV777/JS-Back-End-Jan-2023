<main>
  <div class="container">
    <div class="col-9">
      {{#if photos}}
      {{#each photos}}
       {{> photo username=this.owner.username}}
      {{/each}}
      {{else}}
        <!--If there are no photo yet!-->
        <article class="not-available-photo">
          <h1>No photo posts yet.</h1>
        </article>
      {{/if}}
    </div>
  </div>
</main>